{
    "name": "laramate/structured-document",
    "description": "Structured Documents for Laravel",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Laramate",
            "email": "coding@laramate.de",
            "homepage": "https://www.laramate.de/",
            "role": "Team"
        },
        {
            "name": "Tobi Kivelip",
            "email": "tkivelip@gmail.com",
            "homepage": "https://www.laramate.de/",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3",
        "laravel/laravel": "^5.8",
        "laramate/facade-mapper": "dev-master",
        "laramate/tag": "dev-master",
        "laramate/flex-properties": "dev-master",
        "mindtwo/laravel-auto-create-uuid": "^1.0",
        "mindtwo/laravel-enumerable": "^2.0",
        "spatie/laravel-blade-x": "^2.2",
        "spatie/laravel-medialibrary": "^7.6"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.2",
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Laramate\\StructuredDocument\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laramate\\StructuredDocument\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laramate\\StructuredDocument\\Providers\\DocumentServiceProvider"
            ]
        }
    }
}
